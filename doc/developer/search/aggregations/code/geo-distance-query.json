"aggregations": {
    "distance": {
        "geoDistance": {
            'field': "data.cityLocation",
            'unit': "km",
            'origin': {
                'lat': "90.0",
                'lon': "0.0"
            },
            'ranges': [ { 'from': 0, 'to': 1200 }, { 'from': 1200, 'to': 4000 }, { 'from': 4000, 'to': 12000 }, {'from': 12000} ]
        }
    }
}
